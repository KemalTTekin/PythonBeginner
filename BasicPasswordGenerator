import random
char = 'abcdefghijklmnopqrstuvwxyz123456789ABCDEFGHİJKLMNOPQRSTUVWXYZ!?!^@£$½*&'
number = input('Number;')
number = int(number)
length = int(input('length; '))
for i in range(number):
    password = ''
    for x in range(length) :
        password += random.choice(char)
    print(password)
